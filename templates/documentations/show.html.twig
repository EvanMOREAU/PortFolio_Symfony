{% extends 'base.html.twig' %}

{% block title %}Documentations{% endblock %}

{% block body %}
    <h1>Documentations</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ documentation.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ documentation.name }}</td>
            </tr>
            <tr>
                <th>Pdf</th>
                <td>{{ documentation.pdf }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_documentations_index') }}">back to list</a>

    <a href="{{ path('app_documentations_edit', {'id': documentation.id}) }}">edit</a>

    {{ include('documentations/_delete_form.html.twig') }}
{% endblock %}
